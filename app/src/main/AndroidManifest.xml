<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mwguy.vezdecod_20_mobile">
    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.Vezdecod20mobile">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.AddRecordActivity"/>
        <activity android:name=".activities.AddCategoryActivity"/>
        <activity android:name=".activities.ShowRecordActivity"/>
        <activity android:name=".activities.EditRecordActivity"/>
    </application>
</manifest>
